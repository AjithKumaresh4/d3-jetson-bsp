d3-dtb-y += d3-xavier-cic-imx390rcm.dtb
d3-dtb-y += d3-xavier-cic-ov10640.dtb
d3-dtb-y += d3-xavier-cic-vg6768.dtb
d3-dtb-y += d3-xavier-cic-ar0820.dtb

# Xavier 2xOcto
